{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "description",
        "status",
        "location",
        "department_handle"  
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Lawsuit"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "Lawsuit description"
            ]
        },
        "status": {
            "type": "string",
            "default": "",
            "title": "The status Schema",
            "examples": [
                "open"
            ]
        },
        "location": {
            "type": "string",
            "default": "",
            "title": "The location Schema",
            "examples": [
                "location"
            ]
        },
        "department_handle": {
            "type": "string",
            "default": "",
            "title": "The department_handle Schema",
            "examples": [
                "any department"
            ]
        },
        "created_at": {
            "type": "string",
            "default": "",
            "title": "The created_at Schema",
            "examples": [
                "2020-05-20T12:00:00.000Z"
            ]
        },
        "updated_at": {
            "type": "string",
            "default": "",
            "title": "The updated_at Schema",
            "examples": [
                "2020-05-20T12:00:00.000Z"
            ]
        }
    },
    "examples": [{
        "title": "Lawsuit",
        "description": "Lawsuit description",
        "status": "open",
        "location": "location",
        "department_handle": "any department",
        "created_at": "2020-05-20T12:00:00.000Z",
        "updated_at": "2020-05-20T12:00:00.000Z"
    }]
}