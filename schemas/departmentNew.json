{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "handle",
        "name",
        "num_employees",
        "description"
    ],
    "properties": {
        "handle": {
            "type": "string",
            "default": "",
            "title": "The handle Schema",
            "examples": [
                "health"
            ]
        },
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Health Care Law Department"
            ]
        },
        "num_employees": {
            "type": "integer",
            "default": 0,
            "title": "The num_employees Schema",
            "examples": [
                20
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "Department taking care of health-related lawsuits"
            ]
        }
    },
    "examples": [{
        "handle": "health",
        "name": "Health Care Law Department",
        "num_employees": 20,
        "description": "Department taking care of health-related lawsuits"
    }]
}