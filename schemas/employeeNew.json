{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "additionalProperties":false,
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "paralegal"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "asdfg1234"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "employee"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "employeelast"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "paralegal@employee.com"
            ]
        },
        "isAdmin":{
            "type":"boolean",
            "default":false,
            "title":"The isAdmin Schema",
            "examples":[
                false
            ]
        }
    },
    "examples": [{
        "username": "paralegal",
        "password": "asdfg1234",
        "firstName": "employee",
        "lastName": "employeelast",
        "email": "paralegal@employee.com",
        "isAdmin":false
    }]
}